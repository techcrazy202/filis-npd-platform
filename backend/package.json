    "db:seed": "tsx src/scripts/seed-database.ts",
    "db:setup-tiers": "tsx src/scripts/setup-tiers.ts",
    "db:setup-system": "tsx src/scripts/setup-system.ts",
    "data:import": "tsx src/scripts/data-migration.ts",
    "setup:complete": "tsx src/scripts/complete-setup.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
